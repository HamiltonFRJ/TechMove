function salvaDados (saveDados) {
    localStorage.setItem('lndDados', JSON.stringify (saveDados));
}

function addDados () {
    let strNome = document.getElementById('nomec').value;
    let strEmail = document.getElementById('emailc').value;
    let strData = document.getElementById('datac').value;
    let strNumeroVeiculo = document.getElementById('numerov').value;
    let strDescricao = document.getElementById('avaliacaoc').value;
    
    let novaAv = {
        nome: strNome,
        email: strEmail,
        data: strData,
        numeroVeiculo: strNumeroVeiculo,
        descricao: strDescricao
    };

    objDados.usuarios.push (novaAv);
    salvaDados (objDados);

}

function imprimeDados () {
